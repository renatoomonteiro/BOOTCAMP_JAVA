
public class OperadorUnario {

	public static void main(String[] args) {

		int primeiroValor = 3, segundoValor = 3;
		
		//Pós incremento:
		
		System.out.println(primeiroValor++);
		System.out.println(primeiroValor);
		
		//Pré incremento:
		System.out.println();
		
		System.out.println(++segundoValor);
		System.out.println(segundoValor);


	}

}
